{% extends 'admin_base.html.twig' %}

{% block title %}Quizs{% endblock %}

{% block body %}
    <h1>Quiz</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ Quiz.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ Quiz.image }}</td>
            </tr>
            <tr>
                <th>Incorrect1</th>
                <td>{{ Quiz.incorrect1 }}</td>
            </tr>
            <tr>
                <th>Incorrect2</th>
                <td>{{ Quiz.incorrect2 }}</td>
            </tr>
            <tr>
                <th>Correct</th>
                <td>{{ Quiz.correct }}</td>
            </tr>
            <tr>
                <th>Reponse</th>
                <td>{{ Quiz.reponse ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_Quizs_index') }}">back to list</a>

    <a href="{{ path('app_Quizs_edit', {'id': Quiz.id}) }}">edit</a>

    {{ include('Quizs/_delete_form.html.twig') }}
{% endblock %}
